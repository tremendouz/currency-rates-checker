<div class="table-responsive">
    <table class="table table-bordered table-condensed table-striped table-hover sortable">
        @if (Model.Count == 0)
        {
            <tr>
                <td colspan="10">No Record's found.</td>
            </tr>

        }
        else
        {
            <thead>
                <tr>
                    <th>No</th>
                    <th>EffectiveDate</th>
                    <th>Mid</th>
                </tr>
            </thead>

            foreach (var item in Model)
            {
                <tr>
                    <td>@item.No</td>
                    <td>@item.EffectiveDate</td>
                    <td>@item.Mid</td>
                </tr>

            }
        }
    </table>
</div>